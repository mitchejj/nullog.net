{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "outdated-check": "ncu",
    "dev": "npm-run-all --parallel dev:11ty watch:css",
    "watch:css": "onchange -i 'src/_assets/css/*.css' -- npm run dev:css",
    "dev:11ty": "ELEVENTY_ENV=development eleventy --serve",
    "dev:css": "NODE_ENV=DEV parcel build src/_assets/css/* --out-dir _site_/assets/ --no-minify --no-source-maps",
    "build": "npm-run-all -s prod:*",
    "prod:11ty": "ELEVENTY_ENV=production eleventy --output=./build",
    "prod:parcel": "NODE_ENV=production parcel build src/_assets/css/* --out-dir ./build/assets --no-source-maps --no-minify --detailed-report 10",
    "serve": "npx eleventy --serve",
    "debug": "DEBUG=* npx eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@toycode/markdown-it-class": "^1.2.4",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.1",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.1.10",
    "postcss-modules": "^3.2.2",
    "postcss-nested": "^4.2.3",
    "tailwindcss": "^1.9.6"
  },
  "dependencies": {
    "debug": "^4.3.1"
  }
}
