<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ metadata.title}}: {{ title }}</title>
    <meta name="title" content="" >
    <meta name="description" content="{{ metadata.subtitle }}">
    <!--
        <link rel="icon" href="data:;base64,iVBORw0KGgo=">
    --!>
    <link rel="icon" type="image/png" sizes="32x32"
                                      href="assets/img/favicon-32x32.png">
    <meta name="twitter:card" content="summary"> 
    <meta name="twitter:site" content="@mitchejj"> 
    <meta name="twitter:title" content="{{ metadata.title}}: {{ title }}"> 
    <meta name="twitter:description" content="{{ metadata.subtitle }}"> 
{{# if meta.image.url }}
    <meta name="twitter:image" content="{{ meta.image.url }}">
    <link rel="preload" href="{{ meta.image.url }}" as="image">
{{ else }}
    <meta name="twitter:image" content="/assets/img/android-chrome-512x512.png">
{{/ if}}
    <meta property="og:site_name" content="{{ metadata.title }}">
    <meta property="og:title" content="{{ title }}">
    <meta property="og:type" content="website">
    <meta property="og:url" content="http://nullog.net/">
    <meta property="og:image" content="/assets/img/android-chrome-512x512.png">
    <style>
    </style>
    <link rel="stylesheet" href="/assets/main.css" media="all" onload="this.media='all'">
</head>
